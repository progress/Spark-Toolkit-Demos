        "OpenEdge.Web.DataObject.DataObjectHandler": {
            "logLevel": "INFO",
            "filters": [
                "ABL_SUBSTITUTE_FORMAT",
                "ERROR_FORMAT",
                {
                    "name": "NAMED_FILE_WRITER",
                    "fileName": "${session.temp-dir}/DOH-A${req.agent}-S${req.session}.log",
                    "appendTo": true
                },
                "REPLACE_TOKENS_FORMAT"
            ]
